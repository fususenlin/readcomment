<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  
    PUBLIC "-//tuckey.org//DTD UrlRewrite 3.1//EN"  
    "http://tuckey.org/res/dtds/urlrewrite3.1.dtd">

<urlrewrite>
    <rule>
        <from>^/([a-zA-Z0-9/-]).action$</from>
        <to>$1.action</to>
    </rule>
    <rule>
        <from>/index.aa</from>
        <to type="redirect">books.action</to>
    </rule>
    <rule>
        <from>^/([_a-zA-Z]+[_0-9a-zA-Z-/]*[_0-9a-zA-Z]+)$</from>
        <to>index.html</to>
    </rule>
    <!-- htmlMode 好好想想 -->
</urlrewrite>